{
    "name": "quilt",
    "displayName": "Quilt",
    "description": "Quilt -- View All Code-Related Information In Your IDE",
    "publisher": "Quilt",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:quilt.helloWorld",
        "onView:quilt.view"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "quilt-container",
                    "title": "Quilt",
                    "icon": "media/logo_v1.svg"
                }
            ]
        },
        "views": {
            "quilt-container": [
                {
                    "type": "webview",
                    "id": "quilt.view",
                    "name": "Quilt"
                }
            ]
        },
        "commands": [
            {
                "command": "quilt.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js",
        "build": "webpack --mode development",
        "start": "webpack --watch"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-syntax-export-default-from": "^7.12.1",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "@types/chroma-js": "^2.1.3",
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/vscode": "^1.52.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "babel-loader": "^8.2.2",
        "babel-preset-env": "^1.7.0",
        "chroma-js": "^2.1.1",
        "clean-webpack-plugin": "^3.0.0",
        "eslint": "^6.8.0",
        "file-loader": "^6.2.0",
        "glob": "^7.1.6",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.5.0",
        "mocha": "^7.1.2",
        "typescript": "^3.8.3",
        "url-loader": "^4.1.1",
        "vscode-test": "^1.3.0",
        "watch": "^1.0.2",
        "webpack": "^5.11.0",
        "webpack-cli": "^4.3.0",
        "webpack-dev-server": "^3.11.1"
    },
    "dependencies": {
        "@sentry/react": "^6.2.3",
        "axios": "^0.21.1",
        "email-validator": "^2.0.4",
        "history": "^4.10.1",
        "lodash": "^4.17.20",
        "pusher-js": "^7.0.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-icons": "^4.1.0",
        "react-loader-spinner": "^4.0.0",
        "react-redux": "^7.2.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-spinners": "^0.10.6",
        "react-transition-group": "^4.4.1",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "styled-components": "^5.2.1",
        "svg-loaders-react": "^2.2.1",
        "vscode-uri": "^3.0.1"
    },
    "extensionDependencies": [
        "vscode.git"
    ]
}
